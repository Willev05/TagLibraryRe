<UserControl x:Class="TagLibraryRe.View.UserControls.TagsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TagLibraryRe.View.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="60" d:DesignWidth="200">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button Width="100" Content="New" Command="{Binding NewTag}"/>
            <Button Width="100" Content="Remove"/>
        </StackPanel>
    </Grid>
</UserControl>
